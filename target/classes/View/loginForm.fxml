<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane fx:id="root" prefHeight="508.0" prefWidth="360.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="lk.ijse.stockmanage.controller.LoginForm">
   <children>
      <Label layoutX="83.0" layoutY="34.0" text="Login Form">
         <font>
            <Font name="System Bold Italic" size="31.0" />
         </font>
      </Label>
      <TextField fx:id="user" layoutX="25.0" layoutY="127.0" prefHeight="35.0" prefWidth="293.0" promptText="User Name" />
      <TextField fx:id="password" layoutX="25.0" layoutY="194.0" prefHeight="35.0" prefWidth="293.0" promptText="Password" />
      <Button layoutX="25.0" layoutY="320.0" mnemonicParsing="false" onAction="#btnLoginOnAction" prefHeight="27.0" prefWidth="293.0" style="-fx-background-color: #3498db;" text="Submit">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <Label layoutX="25.0" layoutY="394.0" text="Don't have an account?" />
      <Label layoutX="180.0" layoutY="394.0" text="Register" textFill="#0966d7">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Label>
   </children>
</AnchorPane>
